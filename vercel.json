{
	"version": 2,
	"builds": [
		{
			"src": "package.json",
			"use": "@vercel/static-build",
			"config": {
				"distDir": "dist"
			}
		},
		{
			"src": "server/index.js",
			"use": "@vercel/node"
		}
	],
	"routes": [
		{
			"src": "/socket.io/(.*)",
			"dest": "server/index.js"
		},
		{
			"src": "/(.*)",
			"dest": "/$1"
		}
	]
}
